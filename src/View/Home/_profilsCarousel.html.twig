<div id="carouselExampleControls" class="carousel slide">
    <h3>Profils</h3>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <div class="row">
                {% for i in 1..3 %}
                <div class="card profil-card col-4">
                    <a href="#">
                        <div class="profil-img-container">
                            <img src="/assets/images/art640.jpg" class="card-img-top profil-img" alt="Photo de profile">
                        </div>
                        <div class="card-body">
                            <h5 class="card-title">José#3226</h5>
                            <p class="card-text">#Menuisier, #Maçon, #Domotique</p>
                        </div>
                    </a>
                </div>
                {% endfor %}
            </div>
        </div>
        {% for i in 0..2 %}
            <div class="carousel-item">
                <div class="row">
                    {% for i in 1..3 %}
                        <div class="card profil-card col-4">
                            <a href="#">
                                <div class="profil-img-container">
                                    <img src="/assets/images/art640.jpg" class="card-img-top profil-img" alt="Photo de profile">
                                </div>
                                <div class="card-body">
                                    <h5 class="card-title">José#3226</h5>
                                    <p class="card-text">#Menuisier, #Maçon, #Domotique</p>
                                </div>
                            </a>
                        </div>
                    {% endfor %}
                </div>
            </div>
        {% endfor %}
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

